package LongJulyContest;

import java.util.Arrays;
import java.util.HashMap;
import java.util.HashSet;
import java.util.Map;
import java.util.Scanner;

public class ChefinaSwaps {

	public static void main(String[] args) {


		try {
			Scanner sc= new Scanner(System.in);
			int t= sc.nextInt();
			while(t-->0)
			{
				int n =sc.nextInt();
				HashSet<Long> hs = new HashSet<>(); 
				HashMap<Long,Long> hmX= new HashMap<Long,Long>();
				HashMap<Long,Long> hmY= new HashMap<Long,Long>();
				
				for(int i=0;i<n;i++) {
					long x= sc.nextLong();
					hs.add(x);
					 if (hmX.containsKey(x))  
			            { 
			                hmX.put(x, hmX.get(x) + 1); 
			            }  
			            else
			            { 
			                hmX.put(x, (long)1); 
			            } 
					  
				}
				for(int i=0;i<n;i++) {
					long y = sc.nextLong();
					hs.add(y);
					if (hmY.containsKey(y))  
		            { 
		                hmY.put(y, hmY.get(y) + 1); 
		            }  
		            else
		            { 
		                hmY.put(y, (long)1); 
		            }
				}
				long arr[] = new long[hs.size()];
				long diff[] = new long[hs.size()];
				
				int j=0;
				boolean flag=true;
				for (Long s :hs) 
		        { 
					Long x= hmX.get(s);
					Long y= hmY.get(s);
					
				   if(x!=null && y!=null )
		           {
					   if(Math.abs(x-y)==0)
						   continue;
					   else if(Math.abs(x-y)%2==0) {
						   diff[j]=Math.abs(x-y);
		        		   arr[j]=s;
		        		   j++;
		        	   }
		        	   else {
		        		   System.out.println(-1);
		        		   flag=false;
		        		   break;
		        	   }
		           }
		           else {
		        	   if(x==null && y%2==0) {
		        		   diff[j]=y;
		        		   arr[j]=s;
		        		   j++;
		        	   }
		        	   else if ( y==null && x%2==0) {
		        		   diff[j]=x;
		        		   arr[j]=s;
		        		   j++;
		        	   }
		        	   else {
		        		   System.out.println(-1);
		        		   flag=false;
		        		   break;
		        	   }	  
		           }
		           
		        }
				if(flag) {
					long sum=0;
					for(int i=0;i<hs.size();i++) {
						sum+=arr[i];
					}
					if(sum==0)
						System.out.println(0);
					
				}
				
					
				System.out.println(Arrays.toString(arr));
		
			}
			
		}
		catch(Exception e) {
			
		}
	}

}
